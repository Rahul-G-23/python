class Kangaroo:
    def __init__(self):
        self.pouch_contents = []

    def put_in_pouch(self, item):
        self.pouch_contents.append(item)

    def __str__(self):
        return f"<Kangaroo pouch_contents={self.pouch_contents}>"

# Test code:
kanga = Kangaroo()
roo = Kangaroo()
kanga.put_in_pouch(roo)

print("Name: Rahul G \n USN:1AY24AI088\n Section: 'O'")
print(kanga)
print(roo)
